mixin rate-button(properties = {})

  - 
    const {
      id = '',
      maxRate = 5,
      selectedRate = 0,
    } = properties

    let n = 0;

  section.rate-button(id = id) 
    - for (let i = 1; i < maxRate + 1; i++)
        if (selectedRate < i)
          .rate-button__star.material-icons= 'star_border'
        else 
          .rate-button__star.material-icons= 'star'
