{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-plusplus" : "off",
        "no-trailing-spaces": "off",
        "max-len":"off",
        "no-unused-expressions":"off",
        "no-return-assign":"off"
    }
}
